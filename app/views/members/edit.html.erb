<h1>Members#Edit</h1>
<p>Find me in app/views/members/edit.html.erb</p>

<%= form_for @member, url:'update' do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  
  <%= f.label :age %><br>
  <%= f.number_field :age %><br><br>
  
  <%= f.submit "Update member", class:"btn btn-primary" %>  
<% end %>